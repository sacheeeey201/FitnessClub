package loops;

public class Pattern7 {

	public static void main(String[] args) {

		for(int i=1;i<=4;i++)
		{
			for(int j=3;j>=i;j--)
			{
				System.out.print(" ");
			}
			for(int k=1;k<=i;k++)
			{
				System.out.print("*");
			}
			for(int l=3;l<=k;l++)
			{
				System.out.println("*");
			}
			System.out.println("");
		}
	}

}
