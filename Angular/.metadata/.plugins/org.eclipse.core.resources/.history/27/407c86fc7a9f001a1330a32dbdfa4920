package string;

import java.util.Scanner;

public class SwappingOfNUMWoThirdVar {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.println("Enter numbers ");
		int a=sc.nextInt();
		int b=sc.nextInt();
		
		a=a+b;
		b=a-b;
		a=a-b;
		
		System.out.println("Swapped "+a+" "+b);

	}

}
